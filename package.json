{
  "name": "app",
  "version": "1.0.50",
  "scripts": {
    "test": "vitest",
    "test:backend": "vitest apps/backend",
    "test:frontend": "vitest --config vitest.config.frontend.ts apps/frontend",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "dev": "bun run --watch apps/backend/src/index.ts",
    "dev:backend": "bun run --watch apps/backend/src/index.ts",
    "dev:frontend": "bunx vite apps/frontend",
    "start": "bun run apps/backend/src/index.ts",
    "prisma:generate": "cd apps/backend && prisma generate",
    "prisma:migrate": "cd apps/backend && prisma migrate dev",
    "prisma:seed": "cd apps/backend && prisma db seed"
  },
  "prisma": {},
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/adapter-pg": "latest",
    "@prisma/client": "latest",
    "dotenv": "^17.3.1",
    "elysia": "latest",
    "pg": "latest",
    "pinia": "^3.0.4",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "latest",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "bun-types": "latest",
    "happy-dom": "^20.7.0",
    "prisma": "latest",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.1.5"
  },
  "workspaces": ["packages/*"],
  "module": "./apps/backend/src/index.ts"
}
