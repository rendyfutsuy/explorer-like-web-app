  "devDependencies": {
    "bun-types": "latest",
    "prisma": "latest",
    "@types/pg": "latest",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
